{
    "version": 2,
    "cli": {
        "defaultCollection": "@nrwl/nest",
        "packageManager": "yarn"
    },
    "defaultProject": "backend",
    "projects": {
        "backend": {
            "root": "apps/backend",
            "sourceRoot": "apps/backend",
            "projectType": "application",
            "prefix": "backend",
            "targets": {
                "build": {
                    "executor": "@nrwl/node:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/backend",
                        "main": "apps/backend/main.ts",
                        "tsConfig": "apps/backend/tsconfig.app.json",
                        "showCircularDependencies": false
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/backend/environments/environment.ts",
                                    "with": "apps/backend/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "executor": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "backend:build"
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/backend/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/backend"],
                    "options": {
                        "jestConfig": "apps/backend/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "frontend": {
            "projectType": "application",
            "root": "apps/frontend",
            "sourceRoot": "apps/frontend/src",
            "prefix": "smplct-view",
            "targets": {
                "build": {
                    "executor": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/frontend",
                        "index": "apps/frontend/src/index.html",
                        "main": "apps/frontend/src/main.ts",
                        "polyfills": "apps/frontend/src/polyfills.ts",
                        "tsConfig": "apps/frontend/tsconfig.app.json",
                        "showCircularDependencies": false,
                        "aot": true,
                        "assets": [
                            "apps/frontend/src/favicon.ico",
                            "apps/frontend/src/assets",
                            "apps/frontend/src/manifest.webmanifest"
                        ],
                        "styles": [
                            "node_modules/onsenui/css/onsenui.css",
                            "node_modules/onsenui/css/onsen-css-components.css",
                            "apps/frontend/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/frontend/src/environments/environment.ts",
                                    "with": "apps/frontend/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "serviceWorker": true,
                            "ngswConfigPath": "apps/frontend/ngsw-config.json"
                        }
                    }
                },
                "serve": {
                    "executor": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "frontend:build",
                        "proxyConfig": "apps/frontend/proxy.conf.json",
                        "host": "0.0.0.0"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "frontend:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "executor": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "frontend:build"
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/frontend/src/**/*.ts",
                            "apps/frontend/src/**/*.html"
                        ]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/frontend"],
                    "options": {
                        "jestConfig": "apps/frontend/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "frontend-e2e": {
            "root": "apps/frontend-e2e",
            "sourceRoot": "apps/frontend-e2e/src",
            "projectType": "application",
            "targets": {
                "e2e": {
                    "executor": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/frontend-e2e/cypress.json",
                        "tsConfig": "apps/frontend-e2e/tsconfig.e2e.json",
                        "devServerTarget": "frontend:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "frontend:serve:production"
                        }
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/frontend-e2e/**/*.{js,ts}"]
                    }
                }
            }
        },
        "shared-interfaces": {
            "root": "libs/shared/interfaces",
            "sourceRoot": "libs/shared/interfaces/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/shared/interfaces/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/shared/interfaces"],
                    "options": {
                        "jestConfig": "libs/shared/interfaces/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "shared-constants": {
            "root": "libs/shared/constants",
            "sourceRoot": "libs/shared/constants/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/shared/constants/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/shared/constants"],
                    "options": {
                        "jestConfig": "libs/shared/constants/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "api-gateway": {
            "root": "apps/api-gateway",
            "sourceRoot": "apps/api-gateway/src",
            "projectType": "application",
            "prefix": "api-gateway",
            "targets": {
                "build": {
                    "executor": "@nrwl/node:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/api-gateway",
                        "main": "apps/api-gateway/src/main.ts",
                        "tsConfig": "apps/api-gateway/tsconfig.app.json",
                        "assets": ["apps/api-gateway/src/assets"]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/api-gateway/src/environments/environment.ts",
                                    "with": "apps/api-gateway/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "executor": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "api-gateway:build"
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/api-gateway/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/api-gateway"],
                    "options": {
                        "jestConfig": "apps/api-gateway/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "users-service": {
            "root": "apps/users-service",
            "sourceRoot": "apps/users-service/src",
            "projectType": "application",
            "prefix": "users-service",
            "targets": {
                "build": {
                    "executor": "@nrwl/node:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/users-service",
                        "main": "apps/users-service/src/main.ts",
                        "tsConfig": "apps/users-service/tsconfig.app.json"
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/users-service/src/environments/environment.ts",
                                    "with": "apps/users-service/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "executor": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "users-service:build"
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/users-service/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/users-service"],
                    "options": {
                        "jestConfig": "apps/users-service/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "partners-service": {
            "root": "apps/partners-service",
            "sourceRoot": "apps/partners-service/src",
            "projectType": "application",
            "prefix": "partners-service",
            "targets": {
                "build": {
                    "executor": "@nrwl/node:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/partners-service",
                        "main": "apps/partners-service/src/main.ts",
                        "tsConfig": "apps/partners-service/tsconfig.app.json"
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/partners-service/src/environments/environment.ts",
                                    "with": "apps/partners-service/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "executor": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "partners-service:build"
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/partners-service/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/partners-service"],
                    "options": {
                        "jestConfig": "apps/partners-service/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "shared-services": {
            "root": "libs/shared/services",
            "sourceRoot": "libs/shared/services/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/shared/services/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/shared/services"],
                    "options": {
                        "jestConfig": "libs/shared/services/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "shared-utils": {
            "root": "libs/shared/utils",
            "sourceRoot": "libs/shared/utils/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/shared/utils/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/shared/utils"],
                    "options": {
                        "jestConfig": "libs/shared/utils/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "shared-api": {
            "root": "libs/shared/api",
            "sourceRoot": "libs/shared/api/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/shared/api/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/shared/api"],
                    "options": {
                        "jestConfig": "libs/shared/api/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        }
    },
    "generators": {
        "@nrwl/angular:application": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress",
            "strict": true
        },
        "@nrwl/angular:library": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest",
            "strict": true
        },
        "@nrwl/angular:component": {
            "style": "scss"
        }
    }
}
