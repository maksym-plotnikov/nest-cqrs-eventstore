    "scripts": {
        "nx": "./node_modules/.bin/nx",
        "start": "nx serve",
        "build": "nx build",
        "test": "nx test",
        "lint": "nx workspace-lint && nx lint",
        "lint:all": "nx run-many --target=lint --projects=frontend,backend,shared-interfaces,shared-constants",
        "lint:styles": "npx stylelint \"apps/frontend/**/*.scss\" --cache --aei --max-warnings=0",
        "e2e": "nx e2e",
