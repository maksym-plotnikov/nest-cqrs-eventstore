{
    "npmScope": "smplct-view",
    "affected": { "defaultBase": "main" },
    "implicitDependencies": {
        "workspace.json": "*",
        "package.json": { "dependencies": "*", "devDependencies": "*" },
        "tsconfig.base.json": "*",
        "tslint.json": "*",
        ".eslintrc.json": "*",
        "nx.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
        }
    },
    "projects": {
        "backend": { "tags": [] },
        "frontend": { "tags": [] },
        "frontend-e2e": { "tags": [], "implicitDependencies": ["frontend"] },
        "shared-interfaces": { "tags": [] },
        "shared-constants": { "tags": [] },
        "api-gateway": { "tags": [] },
        "users-service": { "tags": [] },
        "partners-service": { "tags": [] },
        "shared-services": { "tags": [] },
        "shared-utils": { "tags": [] },
        "shared-api": { "tags": [] }
    }
}
